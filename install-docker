    1  sudo apt-get update
    2  for pkg in docker.io docker-doc docker-compose docker-compose-v2 podman-docker containerd runc; do sudo apt-get remove $pkg; done
    3  # Add Docker's official GPG key:
    4  sudo apt-get update
    5  sudo apt-get install ca-certificates curl gnupg
    6  docker
    7  sudo docker
    8  sudo apt-get install docker-ce docker-ce-cli containerd.io docker-buildx-plugin docker-compose-plugin
    9  sudo apt-get install ca-certificates curl gnupg
   10  sudo apt-get install docker-ce docker-ce-cli containerd.io docker-buildx-plugin docker-compose-plugin
   11  sudo apt-get update
   12  sudo apt-get install docker-ce docker-ce-cli containerd.io docker-buildx-plugin docker-compose-plugin
   13  sudo apt install apt-transport-https ca-certificates curl software-properties-common
   14  curl -fsSL https://download.docker.com/linux/ubuntu/gpg | sudo gpg --dearmor -o /usr/share/keyrings/docker-archive-keyring.gpg
   15  echo "deb [signed-by=/usr/share/keyrings/docker-archive-keyring.gpg] https://download.docker.com/linux/ubuntu $(lsb_release -cs) stable" | sudo tee /etc/apt/sources.list.d/docker.list > /dev/null
   16  sudo apt update
   17  sudo apt install docker-ce docker-ce-cli containerd.io
   18  sudo docker ps
   19  history >>install-docker
